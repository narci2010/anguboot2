{
  "xliffmergeOptions": {
    "srcDir": "src/main/web",
    "genDir": "src/main/web/locale",
    "i18nFile": "messages.xlf",
    "i18nFormat": "xlf",
    "encoding": "UTF-8",
    "defaultLanguage": "en",
    "languages": ["es", "fr"],
    "removeUnusedIds": true,
    "supportNgxTranslate": false,
    "verbose": false,
    "quiet": false
  }
}